{
  "dashboard": {
    "id": null,
    "title": "BrahminyKite - AI Framework Performance",
    "tags": ["brahminykite", "ai", "frameworks"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Framework Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(brahminykite_claims_processed_total[5m])",
            "legendFormat": "{{framework}} - {{result}}"
          }
        ],
        "yAxes": [
          {"label": "Claims/sec", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Verification Accuracy by Framework",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(brahminykite_claims_processed_total{result=\"success\"}[1h]) / rate(brahminykite_claims_processed_total[1h])",
            "legendFormat": "{{framework}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 3,
        "title": "Average Verification Confidence",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(brahminykite_verification_confidence_sum[5m]) / rate(brahminykite_verification_confidence_count[5m])",
            "legendFormat": "{{framework}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "color": {"mode": "continuous-GrYlRd"}
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 4,
        "title": "Verification Duration by Framework",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(brahminykite_verification_duration_seconds_bucket[5m]))",
            "legendFormat": "{{framework}} - 50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(brahminykite_verification_duration_seconds_bucket[5m]))",
            "legendFormat": "{{framework}} - 95th percentile"
          }
        ],
        "yAxes": [
          {"label": "Duration (s)", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Model Inference Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(brahminykite_model_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model_type}} - 95th percentile"
          }
        ],
        "yAxes": [
          {"label": "Inference Time (s)", "min": 0}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 6,
        "title": "Model Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(brahminykite_model_cache_hits_total[5m]) / (rate(brahminykite_model_cache_hits_total[5m]) + rate(brahminykite_model_inference_duration_seconds_count[5m]))",
            "legendFormat": "{{model_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.7},
                {"color": "green", "value": 0.9}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      },
      {
        "id": 7,
        "title": "Framework Error Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (framework) (rate(brahminykite_errors_total[5m]))",
            "legendFormat": "{{framework}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 32}
      },
      {
        "id": 8,
        "title": "Claims by Type and Framework",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (framework, claim_type) (increase(brahminykite_verification_duration_seconds_count[1h]))",
            "legendFormat": "{{framework}} - {{claim_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 32}
      }
    ]
  }
}